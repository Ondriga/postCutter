<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent">

    <View
        android:id="@+id/rectangle_innerRectangle"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/rectangle_innerRectangleMargin"
        android:background="@drawable/rectangle_inner_rectangle_background_transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/rectangle_topLeftCorner"
        android:layout_width="@dimen/rectangle_corner"
        android:layout_height="@dimen/rectangle_corner"
        android:background="@drawable/rectangle_corner"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/rectangle_bottomLeftCorner"
        android:layout_width="@dimen/rectangle_corner"
        android:layout_height="@dimen/rectangle_corner"
        android:background="@drawable/rectangle_corner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/rectangle_topRightCorner"
        android:layout_width="@dimen/rectangle_corner"
        android:layout_height="@dimen/rectangle_corner"
        android:background="@drawable/rectangle_corner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/rectangle_bottomRightCorner"
        android:layout_width="@dimen/rectangle_corner"
        android:layout_height="@dimen/rectangle_corner"
        android:background="@drawable/rectangle_corner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/rectangle_topSide"
        android:layout_width="@dimen/rectangle_longSide"
        android:layout_height="@dimen/rectangle_corner"
        android:background="@drawable/rectangle_horizontal_side"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/rectangle_leftSide"
        android:layout_width="@dimen/rectangle_corner"
        android:layout_height="@dimen/rectangle_longSide"
        android:background="@drawable/rectangle_vertical_side"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/rectangle_bottomSide"
        android:layout_width="@dimen/rectangle_longSide"
        android:layout_height="@dimen/rectangle_corner"
        android:background="@drawable/rectangle_horizontal_side"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/rectangle_rightSide"
        android:layout_width="@dimen/rectangle_corner"
        android:layout_height="@dimen/rectangle_longSide"
        android:background="@drawable/rectangle_vertical_side"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>